<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>QuizApp</display-name>
	
	<filter>
		<filter-name>CorsFilter</filter-name>
		<filter-class>com.fdesign.web.CorsFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>CorsFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	
	<servlet>
	<servlet-name>TestQuestServlet</servlet-name>
	<servlet-class>com.fdesign.servlets.TestQuestServlet</servlet-class>
	</servlet>
	
	<servlet>
	<servlet-name>AssignmentServlet</servlet-name>
	<servlet-class>com.fdesign.servlets.AssignmentServlet</servlet-class>
	</servlet>
	
	<servlet>
	<servlet-name>QuestionServlet</servlet-name>
	<servlet-class>com.fdesign.servlets.QuestionServlet</servlet-class>
	</servlet>
	
	<servlet>
	<servlet-name>QuizServlet</servlet-name>
	<servlet-class>com.fdesign.servlets.QuizServlet</servlet-class>
	</servlet>
	
	<servlet>
	<servlet-name>TestServlet</servlet-name>
	<servlet-class>com.fdesign.servlets.TestServlet</servlet-class>
	</servlet>
	
	<servlet>
	<servlet-name>UserServlet</servlet-name>
	<servlet-class>com.fdesign.servlets.UserServlet</servlet-class>
	</servlet>
	
	<servlet>
	<servlet-name>UserQuizServlet</servlet-name>
	<servlet-class>com.fdesign.servlets.UserQuizServlet</servlet-class>
	</servlet>
	
	<servlet>
	<servlet-name>AuthServlet</servlet-name>
	<servlet-class>com.fdesign.servlets.AuthServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>AuthServlet</servlet-name>
		<url-pattern>/auth/*</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>UserServlet</servlet-name>
		<url-pattern>/users/*</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>UserQuizServlet</servlet-name>
		<url-pattern>/userquiz/*</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>TestServlet</servlet-name>
		<url-pattern>/test/*</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>QuizServlet</servlet-name>
		<url-pattern>/quiz/*</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>QuestionServlet</servlet-name>
		<url-pattern>/question/*</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>AssignmentServlet</servlet-name>
		<url-pattern>/assign/*</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>TestQuestServlet</servlet-name>
		<url-pattern>/testquest/*</url-pattern>
	</servlet-mapping>
	
</web-app>